{
  "name": "aframe-websurfaces",
  "version": "1.1.0",
  "homepage": "https://github.com/ryota-mitarai/aframe-websurfaces",
  "description": "An aframe component for adding interactable web pages to your scene.",
  "repository": "https://github.com/ryota-mitarai/aframe-websurfaces.git",
  "author": "ryota-mitarai",
  "license": "MIT",
  "files": [
    "dist/*.js*",
    "dist/*.ts"
  ],
  "source": "src/websurface.ts",
  "main": "dist/websurface.js",
  "exports": "dist/websurface.modern.js",
  "module": "dist/websurface.module.js",
  "unpkg": "dist/websurface.umd.js",
  "peerDependencies": {
    "aframe": "^1.2.0",
    "three": ">=0.125.0"
  },
  "devDependencies": {
    "@types/aframe": "^1.0.3",
    "@types/three": "^0.127.0",
    "microbundle": "^0.13.0",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "build": "microbundle --globals aframe=AFRAME,three=THREE --external aframe,three",
    "dev": "microbundle watch"
  },
  "keywords": [
    "aframe",
    "a-frame",
    "css3d",
    "websurface"
  ]
}
